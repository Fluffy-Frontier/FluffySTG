/obj/item/clothing/mask/gas/sechailer/clown
	var/list/unique_text

/obj/item/clothing/mask/gas/sechailer/clown/proc/play_sound_eff()
	return

/obj/item/clothing/mask/gas/sechailer/clown/play_phrase(mob/user, datum/hailer_phrase/phrase)
	if(!COOLDOWN_FINISHED(src, hailer_cooldown))
		return
	COOLDOWN_START(src, hailer_cooldown, 3 SECONDS)
	var/selected_text = pick(unique_text)
	user.say(selected_text)
	user.audible_message("[user]'s Compli-o-Nator: <font color='red' size='4'><b>[selected_text]</b></font>")
	play_sound_eff()

